#include "cache.h"
#include <list>

#define TAGSIZE 12
#define TRUNCATE(x, nbits) (((x) & ((1 << (nbits)) - 1)))
#define SAMEPAGE(lineaddr1, lineaddr2) ((((lineaddr1) ^ (lineaddr2)) >> LOG2_BLOCK_SIZE) == 0)

#define DEGREE 16

// For Page Access History Table
#define PRINT_INTERVAL 255
#define NPAHTES 256
#define SHIFTSIZE 6
#define IDSIZE 12

// For Sandbox
#define NSBES 64

// For Prefetch Offset Candidate
#define NPOCS DEGREE

// For Best Offset List
#define NBOLES 64

// For Prefetch Filter
#define NPFES 256

// For Useless Prefetch
#define NULES 64

// #define TEST
// #define MATPLOT
// #define BOLTEST
// #define FILTER

#ifdef MATPLOT
#define MIN_CYCLE 0
#endif

#ifdef BOLTEST
#define MIN_CYCLE 0
#endif

struct pahte {
  uint16_t tag; // 12 bits
  uint8_t init; // 6 bits
  uint8_t next; // 6 bits
  bool valid;   // 1 bit
  uint8_t lru;  // 8 bits (for 256 entries)

  pahte() : tag(0), init(0), next(0), valid(false), lru(0) {}
};

pahte paht[NUM_CPUS][NPAHTES];

struct sbe {
  uint16_t id;     // 12 bits
  uint16_t tag;    // 12 bits
  uint16_t offset; // 6 bits

  sbe() : id(0), tag(0), offset(0) {}
};

typedef std::list<sbe> sandbox_list;
typedef sandbox_list::reverse_iterator sandbox_iter;

sandbox_list sandbox[NUM_CPUS];

typedef std::list<uint8_t> offset_list;
typedef offset_list::reverse_iterator offset_iter;

struct poce {
  uint8_t po;       // 6 bits
  uint8_t positive; // 6 bits
  uint8_t negative; // 6 bits

  poce() : po(0), positive(0), negative(0) {}
};

typedef std::list<poce> poc_list;
typedef poc_list::reverse_iterator poc_iter;

struct bo {
  uint16_t id;
  uint8_t po;
  uint8_t score;

  bo() : id(0), po(0), score(0) {}
};

typedef std::list<bo> bo_list;
typedef bo_list::iterator bo_iter;
typedef bo_list::reverse_iterator bo_riter;

bo_list bol[NUM_CPUS];

struct pf {
  uint16_t id;
  uint8_t po;
  uint16_t tag;
  uint8_t offset;

  pf() : id(0), po(0), tag(0), offset(0) {}
};

typedef std::list<pf> pf_list;
typedef pf_list::iterator pf_iter;
typedef pf_list::reverse_iterator pf_riter;

pf_list pfl[NUM_CPUS];

struct ul {
  uint16_t tag;
  uint8_t offset;
  uint8_t end;

  ul() : tag(0), offset(0), end(0) {}
};

typedef std::list<ul> ul_list;
typedef ul_list::iterator ul_iter;
typedef ul_list::reverse_iterator ul_riter;

ul_list ull[NUM_CPUS];

struct pol {
  uint16_t id;
  uint8_t po;

  pol() : id(0), po(0) {}
};

pol prefetch_offset_list[NUM_CPUS][DEGREE];

uint8_t offset_search_count[NUM_CPUS];
bool is_hit_on_page_access_history_table[NUM_CPUS];
uint8_t page_access_history_table_entry[NUM_CPUS];
uint8_t current_degree[NUM_CPUS];

void test_useless_prefetch_list(uint8_t cpu) {
  for (ul_riter riter = ull[cpu].rbegin(); riter != ull[cpu].rend(); ++riter) {
    ul ul_entry = *riter;
    printf("ULL TAG: %03x, OFFSET: %d, END: %d\n", ul_entry.tag, ul_entry.offset, ul_entry.end);
  }
  printf("\n");
}

void push_entry_into_ull(uint8_t cpu, ul &ul_entry) {
  ull[cpu].push_back(ul_entry);
  if (ull[cpu].size() > NULES) {
    ull[cpu].pop_front();
  }
}

void insert_useless_prefetch(uint8_t cpu, uint64_t cl_address) {
  // cl_address에는 useless prefetch 주소가 와야한다. 그 프리페치 주소를 tag, offset으로 분리한다.
  // tag가 일치하는 엔트리가 존재하면 해당 엔트리를 최상단으로 올린다. 이 때 end보다 현재 오프셋이
  // 크면 갱신한다. 없으면 엔트리를 정의한 뒤 리스트에 밀어넣는다.

  uint8_t offset = TRUNCATE(cl_address, LOG2_BLOCK_SIZE);
  uint64_t page = cl_address >> LOG2_BLOCK_SIZE;
  uint16_t tag = TRUNCATE(page, TAGSIZE);
  bool exist = false;

  for (ul_iter iter = ull[cpu].begin(); iter != ull[cpu].end(); ++iter) {
    ul *useless_prefetch_entry = &(*iter);
    if (useless_prefetch_entry->tag == tag) {
      ul recent_entry;
      recent_entry.tag = useless_prefetch_entry->tag;
      recent_entry.offset = useless_prefetch_entry->offset;
      if (offset > useless_prefetch_entry->end) {
        recent_entry.end = offset;
      }

      // 엔트리 삭제
      ull[cpu].erase(iter);

      // 엔트리 최상위에 추가
      push_entry_into_ull(cpu, recent_entry);

      exist = true;
      break;
    }
  }

  if (!exist) {
    ul useless_prefetch_entry;
    useless_prefetch_entry.tag = tag;
    useless_prefetch_entry.offset = offset;
    push_entry_into_ull(cpu, useless_prefetch_entry);
  }

  // #ifdef FILTER
  //   test_useless_prefetch_list(cpu);
  // #endif
}

void insert_prefetch(uint8_t cpu, uint16_t id, uint8_t po, uint64_t cl_address) {
  // cl_address에는 프리페치 주소가 와야한다. 그 프리페치 주소를 tag, offset으로 분리하고
  // 엔트리를 정의한 뒤 리스트에 밀어넣는다.
  uint8_t offset = TRUNCATE(cl_address, LOG2_BLOCK_SIZE);
  uint64_t page = cl_address >> LOG2_BLOCK_SIZE;
  uint16_t tag = TRUNCATE(page, TAGSIZE);

  pf prefetch_filter_entry;
  prefetch_filter_entry.id = id;
  prefetch_filter_entry.po = po;
  prefetch_filter_entry.tag = tag;
  prefetch_filter_entry.offset = offset;

  pfl[cpu].push_back(prefetch_filter_entry);
  if (pfl[cpu].size() > NPFES) {
    pfl[cpu].pop_front();
  }
}

bool pf_hit(uint8_t cpu, uint64_t cl_address) {
  // 주소가 있는지 확인하는 것이므로 tag, offset이 확인 대상
  // id, po와는 무관하다.
  uint8_t offset = TRUNCATE(cl_address, LOG2_BLOCK_SIZE);
  uint64_t page = cl_address >> LOG2_BLOCK_SIZE;
  uint16_t tag = TRUNCATE(page, TAGSIZE);
  bool exist = false;

  for (pf_riter riter = pfl[cpu].rbegin(); riter != pfl[cpu].rend(); ++riter) {
    pf prefetch_filter_entry = *riter;
    if (prefetch_filter_entry.tag == tag && prefetch_filter_entry.offset == offset) {
      exist = true;
      break;
    }
  }

  return exist;
}

